        tools:layout="@layout/fragment_returns" />
    <fragment
        android:id="@+id/closedFragment"
        android:name="com.example.redesignandroidcashierkt.ClosedFragment"
        android:label="اغلاق اليومية"
        tools:layout="@layout/fragment_closed" />
    <fragment
        android:id="@+id/reportsFragment"
